
<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="settings_type" format="enum">
        <enum name="undefined" value="-1" />
        <enum name="global" value="0" />
        <enum name="secure" value="1" />
        <enum name="system" value="2" />
    </attr>

    <attr name="differing_key" format="string|reference" />
    <attr name="dialog_layout" format="integer|reference" />
    <attr name="dangerous" format="boolean|reference" />
    <attr name="low_api" format="integer|reference" />
    <attr name="high_api" format="integer|reference" />
    <attr name="minValue" format="integer|reference" />
    <attr name="maxValue" format="integer|reference" />
    <attr name="scale" format="float|reference" />
    <attr name="units" format="string|reference" />
    <attr name="verifier" />
    <attr name="visibility_verifier" />
    <attr name="enabled_verifier" />
    <attr name="activity_class" />
    <attr name="dialog_class" />
    <attr name="icon_color" format="color|reference" />

    <declare-styleable name="DangerousPreference">
        <attr name="dangerous" />
    </declare-styleable>

    <declare-styleable name="SecureSwitchPreference">
        <attr name="enabled_value" format="string|reference" />
        <attr name="disabled_value" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="SecureSeekBarPreference">
        <attr name="minValue" />
        <attr name="maxValue" />
        <attr name="scale" />
        <attr name="units" />
    </declare-styleable>

    <declare-styleable name="SecureEditTextPreference">
        <attr name="android:inputType" />
    </declare-styleable>

    <declare-styleable name="SecureListPreference">
        <attr name="verifier" />
        <attr name="entries" />
        <attr name="android:entries" />
        <attr name="entryValues" />
        <attr name="android:entryValues" />
    </declare-styleable>

    <declare-styleable name="BaseSecurePreference">
        <attr name="settings_type" />
        <attr name="differing_key" />
        <attr name="dangerous" />
        <attr name="visibility_verifier" />
        <attr name="enabled_verifier" />
        <attr name="low_api" />
        <attr name="high_api" />
        <attr name="use_shared_prefs" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="VerifiedPreference">
        <attr name="visibility_verifier" />
        <attr name="enabled_verifier" />
        <attr name="low_api" />
        <attr name="high_api" />
    </declare-styleable>

    <declare-styleable name="ActivityPreference">
        <attr name="activity_class" />
    </declare-styleable>

    <declare-styleable name="ColorPreference">
        <attr name="icon_color" />
    </declare-styleable>

    <declare-styleable name="BlacklistPreference">
        <attr name="additional_keys" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="NavigationPreference">
        <attr name="navigation_action" format="reference" />
    </declare-styleable>

    <declare-styleable name="ManageQSPreference">
        <attr name="manage_component" format="string" />
    </declare-styleable>

    <declare-styleable name="ShowDialogPreference">
        <attr name="dialog_class" />
    </declare-styleable>

    <declare-styleable name="LinkPreference">
        <attr name="link" format="string" />
    </declare-styleable>
</resources>